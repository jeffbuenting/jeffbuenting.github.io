---
layout: default
---

<div class="home">

  <h1 class="page-heading">Welcome </h1>
  
</div>

<div id="leftcolumn-Title"/>
	<h1>Blog Posts</h1>
</div>

<div id="leftcolumn"/>
  <ul class="post-list">
    {% for post in site.posts %}
      <li>
        <span class="post-meta">{{ post.date | date: "%b %-d, %Y" }}</span>

        <h2>
          <a class="post-link" href="{{ post.url | prepend: site.baseurl }}">{{ post.title }}</a>
        </h2>
      </li>
    {% endfor %}
  </ul>

  <p class="rss-subscribe">subscribe <a href="{{ "/feed.xml" | prepend: site.baseurl }}">via RSS</a></p>

</div>

<div id="Articles-Title">
	
	<h1>Articles</h1>
</div>


<div id="Articles"/>
  
	
	<a class="post-link" href="{{ "Articles/Docker" | prepend: site.baseurl }}">Docker</a>
	<a class="post-link" href="{{ "Articles/GitHubProjects" | prepend: site.baseurl }}">GitHub Projects</a>
	<a class="post-link" href="{{ "Articles/Kubernetes" | prepend: site.baseurl }}">Kubernetes Container Orchestration</a>
	<a class="post-link" href="{{ "Articles/CRM" | prepend: site.baseurl }}">MicroSoft CRM</a>
	<a class="post-link" href="{{ "Articles/PowershellDSC" | prepend: site.baseurl }}">Powershell DSC (Desired State Configuration)</a>
	<a class="post-link" href="{{ "Articles/PowershellForGit" | prepend: site.baseurl }}">Powershell for GIT</a>
	<a class="post-link" href="{{ "Articles/PowershellPester" | prepend: site.baseurl }}">Powershell Pester</a>
	<a class="post-link" href="{{ "Articles/WindowsContainers" | prepend: site.baseurl }}">Windows Containers</a>
	
	
</div>

 
 

